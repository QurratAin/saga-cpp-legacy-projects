#set nologscale 
plot   'force.dat' index 0:0 u 1:3:4:xticlabels(2) with yerrorbars t  "Sync RE"; replot'force.dat' using 1:3 w l t ""
replot 'force.dat' index 1:1 u 1:3:4:xticlabels(2) with yerrorbars t  "Async-Cent RE"; replot'force.dat' using 1:3 w l t ""
replot 'force.dat' index 2:2 u 1:3:4:xticlabels(2) with yerrorbars t  "Async-Decent RE"; replot'force.dat' using 1:3 w l t ""
set xlabel "Number of Replicas (Number of Exchanges)"
set ylabel "Time to Completion (seconds)"




