set terminal postscript eps enhanced color solid rounded 
set output "plotname.eps" 
!epstopdf plotname.eps && rm plotname.eps 

set key top right samplen 2
#set nologscale 
plot   'scale_up.dat' index 0:0 u 1:3:4:xticlabels(2) with yerrorbars t  "Sync RE"; replot'scale_up.dat' using 1:3 w l t ""
replot 'scale_up.dat' index 1:1 u 1:3:4:xticlabels(2) with yerrorbars t  "Async-Cent RE"; replot'scale_up.dat' using 1:3 w l t ""
replot 'scale_up.dat' index 2:2 u 1:3:4:xticlabels(2) with yerrorbars t  "Async-Decent RE"; replot'scale_up.dat' using 1:3 w l t ""
set xlabel "Number of Replicas"
set ylabel "Time to Completion (seconds)"
